<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gamepoint.com.bintracking.activity.DashboardActivity">


    <ImageView
        android:id="@+id/eventadd"
        android:layout_width="166dp"
        android:layout_height="131dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="216dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/icon_password" />

    <ImageView
        android:id="@+id/mapadd"
        android:layout_width="120dp"
        android:layout_height="133dp"
        android:layout_marginStart="30dp"
        app:layout_constraintBottom_toTopOf="@+id/eventadd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.741"
        app:srcCompat="@drawable/trackbin" />

    <ImageView
        android:id="@+id/chatadd"
        android:layout_width="176dp"
        android:layout_height="131dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.304"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.311"
        app:srcCompat="@drawable/logo_chat" />

    <ImageView
        android:id="@+id/blogview"
        android:layout_width="174dp"
        android:layout_height="124dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chatadd"
        app:layout_constraintVertical_bias="0.225"
        app:srcCompat="@drawable/icon_blog" />
</androidx.constraintlayout.widget.ConstraintLayout>